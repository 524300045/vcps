            android:windowSoftInputMode="stateHidden|stateUnchanged" >
        </activity>
        <activity
            android:name="com.wologic.ui.DataSetActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|stateUnchanged" >
        </activity>
        <activity
            android:name="com.wologic.ui.OperationRuKuActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.wologic.ui.OperationChuKuActivity"
            android:screenOrientation="portrait" >
