            android:name="com.wologic.ui.OpenFileDialogActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.EDIT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wologic.ui.SaveFileDialogActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.EDIT" />
            </intent-filter>
        </activity>
    </application>

</manifest>